{
	"$schema": "https://json.schemastore.org/prettierrc",

	"printWidth": 160,

	"tabWidth": 4,
	"useTabs": true,

	"endOfLine": "lf",

	"semi": true,
	"singleQuote": false,

	"trailingComma": "all",
	"arrowParens": "always",
	"bracketSpacing": true,
	"quoteProps": "consistent",

	"singleAttributePerLine": true,
	"vueIndentScriptAndStyle": true
}
